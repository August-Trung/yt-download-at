{
	"name": "yt-downloader-backend",
	"version": "3.0.0",
	"description": "Backend server using native yt-dlp binary",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"dev": "nodemon index.js",
		"postinstall": "curl -L https://github.com/yt-dlp/yt-dlp/releases/latest/download/yt-dlp -o ./yt-dlp && chmod +x ./yt-dlp"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"ytpl": "^2.3.0"
	},
	"devDependencies": {
		"nodemon": "^3.0.1"
	}
}
